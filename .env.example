# Supabase
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Prisma Database URL
# For connection pooling, add query parameters:
# ?connection_limit=10&pool_timeout=20&connect_timeout=10&socket_timeout=10
# Example: postgresql://user:password@host:5432/database?connection_limit=10&pool_timeout=20
DATABASE_URL=your-database-url

# Postgres URLs from Vercel or Supabase
POSTGRES_URL=your-postgres-url
POSTGRES_PRISMA_URL=your-postgres-prisma-url
POSTGRES_URL_NON_POOLING=your-postgres-url-non-pooling

# Direct Postgres Connection (for migrations)
DATABASE_POSTGRES_URL=your-postgres-url-non-pooling

# Postgres Credentials
POSTGRES_HOST=your-postgres-host
POSTGRES_DATABASE=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-postgres-password

# Connection Pool Configuration (embedded in DATABASE_URL)
# connection_limit: Maximum number of connections (default: 10)
# pool_timeout: Seconds to wait for connection from pool (default: 20)
# connect_timeout: Seconds to wait for new connection (default: 10)
# socket_timeout: Seconds to wait for query response (default: 10)

